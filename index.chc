<!DOCTYPE html>
<html>

<head> 
<title>Peak</title>
<link rel="stylesheet" href="urmom.css">
</head>

<body>
<nav>
<ul>
@
const char *dir_path = "src";
const char *list;
File_Paths path = {0};
if(!read_entire_dir(dir_path, &path)) return 1;
for (int i = 0; i < path.count; ++i) {
   if(strcmp(path.items[i],"..") == 0 || strcmp(path.items[i],".") == 0)
       continue;
    else {
       list = path.items[i];
       char *dot = strchr(list, '.');
       if (dot != NULL && strcmp(dot + 1, "html") == 0) {
           char strip[100];
           if (strlen(list) > 5) {
               strncpy(strip, list, strlen(list) - 5);
               strip[strlen(list) - 5] = '\0';
           } else {
               strcpy(strip, list);
           }
           printf("<li><a href= '%s/%s'>%s</a></li>", dir_path, list, strip);
       }
   }
}
@
</nav>

<script href = "main.js"></script>
</body>
</html>
